{
	"name": "@svelterialjs/form",
	"version": "0.0.0",
	"description": "Svelte Materialify fork with ts support and updated scss.",
	"license": "MIT",
	"scripts": {
		"build": "concurrently \"yarn:build:js\" \"yarn:build:minimal\"",
		"build:js": "rollup -c",
		"build:minimal": "node preprocess.js",
		"check": "svelte-check --workspace src/components",
		"prepublishOnly": "concurrently \"yarn:build:js\" \"yarn:build:minimal\"",
		"test": "jest",
		"version": "standard-changelog && git add CHANGELOG.md"
	},
	"files": [
		"compiled",
		"dist",
		"src"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/svelterialjs/svelterial.git",
		"directory": "packages/svelte-material"
	},
	"author": "abdo643-HULK",
	"homepage": "https://github.com/abdo643-HULK/svelte-material-components",
	"bugs": {
		"url": "https://github.com/abdo643-HULK/svelte-material-components/issues"
	},
	"main": "compiled/index.js",
	"module": "compiled/index.mjs",
	"keywords": [
		"svelte",
		"sapper",
		"svelte-kit",
		"svelte material",
		"material",
		"material ui",
		"material-components",
		"component-library",
		"svelte-material",
		"ui"
	],
	"peerDependencies": {
		"svelte": ">=3.29.4"
	},
	"svelte": "dist/index.js",
	"devDependencies": {
		"@sveltejs/kit": "^1.0.0-next.1",
		"svelte": "^3.41.0",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5"
	}
}
