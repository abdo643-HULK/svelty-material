{
	"name": "@abdo1504/svelte-material-components",
	"version": "1.0.14",
	"description": "Svelte Materialify fork with ts support and updated scss.",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "sveltekit",
		"dev:check": "npm-run-all --parallel sveltekit check:watch",
		"package": "svelte-kit package",
		"build": "svelte-kit build",
		"check": "svelte-check --tsconfig ./tsconfig.json --threshold error",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch --threshold error --ignore .svelte-kit",
		"sveltekit": "svelte-kit dev"
	},
	"files": [
		"src"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/svelterialjs/svelterial.git",
		"directory": "packages/svelte-material"
	},
	"author": "abdo643-HULK",
	"homepage": "https://github.com/abdo643-HULK/svelte-material-components",
	"bugs": {
		"url": "https://github.com/abdo643-HULK/svelte-material-components/issues"
	},
	"keywords": [
		"svelte",
		"sapper",
		"svelte-kit",
		"svelte material",
		"material",
		"material ui",
		"material-components",
		"component-library",
		"svelte-material",
		"ui"
	],
	"peerDependencies": {
		"svelte": ">=3.29.4"
	},
	"svelte": "dist/index.js",
	"devDependencies": {
		"@rollup/plugin-typescript": "^8.3.0",
		"@sveltejs/kit": "^1.0.0-next.216",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.33",
		"build-esm": "^4.2.2",
		"esbuild": "^0.12.29",
		"npm-run-all": "^4.1.5",
		"rollup": "^2.63.0",
		"rollup-plugin-esbuild": "^4.8.2",
		"sass": "^1.45.2",
		"svelte": "^3.44.3",
		"svelte-check": "^2.2.11",
		"svelte-preprocess": "^4.10.1",
		"svelte2tsx": "^0.4.12",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4",
		"vite": "^2.7.10"
	}
}
